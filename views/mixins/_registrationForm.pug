mixin registrationForm(regform)
  form.form(disabled)

    .form-group
      label(for="mongoId") Database ID
      input.form-control(type="text" id="mongoId" value=regform._id readonly)
    
    .form-group
      label(for="name") Name
      input.form-control(type="text" name="name" id="name" value=regform.name required readonly)

    .form-group
      label(for="email") Email
      input.form-control(type="email" name="email" id="email" value=regform.email required readonly)
    
    .form-group
      label(for="submissionTime") Submission Time
      input.form-control(type="text" name="submissionTime" id="submissionTime" value=h.fmtDate(regform.submissionTime) required readonly)
