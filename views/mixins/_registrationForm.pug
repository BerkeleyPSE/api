mixin registrationForm(regform)

  .row
    .col-xs-12.col-md-4
      form.form

        .form-group
          label(for="mongoId") Database ID
          input.form-control(type="text" id="mongoId" value=regform._id readonly)
        
        .form-group
          label(for="name") Name
          input.form-control(type="text" name="name" id="name" value=regform.name required disabled)

        .form-group
          label(for="email") Email
          input.form-control(type="email" name="email" id="email" value=regform.email required disabled)
        
        .form-group
          label(for="submissionTime") Submission Time
          input.form-control(type="text" name="submissionTime" id="submissionTime" value=h.fmtDate(regform.submissionTime) required disabled)
